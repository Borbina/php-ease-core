{
    "name": "vitexsoftware/ease-core",
    "description": "An PHP Framework for writing Applications",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz",
            "homepage": "http://v.s.cz/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ease\\": "src/Ease/",
            "Ease\\Logger\\": "src/Ease/Logger"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ease\\Local\\": "tests",
            "Test\\Ease\\": "tests/src/Ease",
            "Test\\Ease\\Logger\\": "tests/src/Ease/Logger"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6",
        "ext-pdo": "*",
        "pear/mail": "~1.3",
        "pear/mail_mime": "~1.10"
    },
    "require-dev": {
        "apigen/apigen": ">=4",
        "robmorgan/phinx": "^0.9",
        "phpunit/php-token-stream": ">=1.4",
        "phpunit/phpunit": "^6",
        "phpunit/phpunit-skeleton-generator": "*",
        "mockery/mockery": "^1.1",
        "squizlabs/php_codesniffer": "^3.2",
        "php-coveralls/php-coveralls": "^2",
        "jakub-onderka/php-parallel-lint": "^1",
        "jakub-onderka/php-console-highlighter": "*"
    },
    "type": "library",
    "homepage": "http://v.s.cz/ease.php",
    "license": "GPL-2.0-or-later"
}
